{
    /** The script's entry point. */
    "entrypoints": ["user.ts"],
    /** If you don't want to use manifests, use banner to ***override*** and insert your own string */
    "banner": "",
    /** Your script's name */
    "name": "Example script",
    "version": "1.0",
    /** Namespaces are used to distiguish between 2 scripts of the same name */
    "namespace": "Userscript scripts",
    /** Pretty description */
    "description": "Example script",
    /** URLs to match on running your userscript. */
    "match": ["*://example.org/*"],
    /** URLs to explicitly NOT match for running your userscript. */
    "match-exclude": [],
    /** A list of extra permissions to grant to your script. */
    "grant": [],
    /** When your script will be run on the page. */
    "run-at": "document-end",
}